{
    "name": "temporal-generated-project",
    "version": "0.1.0",
    "private": true,
    "type": "commonjs",
    "scripts": {
        "build": "tsc --build",
        "build.watch": "tsc --build --watch",
        "lint": "eslint .",
        "start": "ts-node src/local/main.ts",
        "start.watch": "nodemon src/local/main.ts",
        "server": "ts-node src/main.ts",
        "server.watch": "nodemon src/main.ts",
        "workflow": "ts-node src/client.ts",
        "cancel-workflow": "ts-node src/cancel-workflow.ts --workflowId",
        "format": "prettier --config .prettierrc 'src/**/*.ts' --write",
        "test": "jest",
        "ci-test": "jest --ci --runInBand --testPathPattern='/tests/ci/'"
    },
    "nodemonConfig": {
        "execMap": {
            "ts": "ts-node"
        },
        "ext": "ts",
        "watch": [
            "src"
        ]
    },
    "dependencies": {
        "@aws-sdk/client-ssm": "3.513.0",
        "@temporalio/activity": "^1.11.6",
        "@temporalio/client": "^1.11.6",
        "@temporalio/common": "^1.11.6",
        "@temporalio/worker": "^1.11.6",
        "@temporalio/workflow": "^1.11.6",
        "@types/luxon": "^3.4.2",
        "axios": "^1.8.4",
        "close-with-grace": "1.3.0",
        "fastify": "4.28.1",
        "luxon": "^3.5.0",
        "nanoid": "3.x"
    },
    "devDependencies": {
        "@babel/preset-env": "^7.26.0",
        "@babel/preset-typescript": "^7.26.0",
        "@jest/globals": "^29.7.0",
        "@temporalio/testing": "^1.11.5",
        "@tsconfig/node18": "^18.2.4",
        "@types/jest": "^29.5.14",
        "@types/mongodb": "^4.0.6",
        "@types/node": "^22.9.1",
        "@types/node-fetch": "^2.5.12",
        "@typescript-eslint/eslint-plugin": "^5.0.0",
        "@typescript-eslint/parser": "^5.0.0",
        "class-validator": "^0.14.1",
        "eslint": "^7.32.0",
        "eslint-config-prettier": "^8.3.0",
        "eslint-plugin-deprecation": "^1.2.1",
        "jest": "^29.7.0",
        "mongodb": "^6.12.0",
        "nodemon": "^3.1.7",
        "prettier": "^2.8.8",
        "source-map-support": "^0.5.21",
        "ts-jest": "^29.2.5",
        "ts-node": "^10.9.2",
        "typescript": "^5.6.3",
        "uuid": "^11.0.5",
        "webpack": "^5.97.1"
    }
}
